{
    "name": "@turbodocx/n8n-nodes-turbodocx",
    "version": "1.0.1",
    "description": "n8n node for TurboDocx API - Document & Presentation Generation and TurboSign digital signatures",
    "license": "MIT",
    "homepage": "https://turbodocx.com",
    "keywords": [
        "n8n-community-node-package",
        "n8n",
        "turbodocx",
        "turbosign",
        "esignature",
        "e-signature",
        "digital-signature",
        "electronic-signature",
        "pdf-signature",
        "document-signing",
        "pdf",
        "signatures",
        "documents",
        "docx",
        "html-to-docx",
        "document-generation",
        "document-automation",
        "workflow",
        "automation",
        "api",
        "pptx",
        "presentations",
        "templates",
        "word",
        "powerpoint"
    ],
    "author": {
        "name": "TurboDocx",
        "email": "team@turbodocx.com"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/turbodocx/n8n-nodes-turbodocx.git"
    },
    "scripts": {
        "build": "n8n-node build",
        "build:watch": "tsc --watch",
        "dev": "n8n-node dev",
        "lint": "n8n-node lint",
        "lint:fix": "n8n-node lint --fix",
        "test": "echo \"No tests configured\" && exit 0",
        "release": "n8n-node release",
        "prepublishOnly": "npm run build"
    },
    "files": [
        "dist"
    ],
    "n8n": {
        "n8nNodesApiVersion": 1,
        "strict": true,
        "credentials": [
            "dist/credentials/TurboDocxApi.credentials.js"
        ],
        "nodes": [
            "dist/nodes/TurboDocx/TurboDocx.node.js"
        ]
    },
    "devDependencies": {
        "@n8n/node-cli": "*",
        "@types/node": "^24.10.1",
        "eslint": "9.32.0",
        "prettier": "3.6.2",
        "release-it": "^19.0.4",
        "typescript": "5.9.2"
    },
    "peerDependencies": {
        "n8n-workflow": "*"
    }
}
